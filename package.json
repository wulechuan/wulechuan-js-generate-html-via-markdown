{
  "name": "@wulechuan/generate-html-via-markdown",
  "author": "wulechuan <wulechuan@live.com>",
  "license": "WTFPL",
  "version": "2.9.2",
  "description": "Yet another tool for converting MarkDown content into corresponding HTML content, but with gorgeous themes applied to the output HTML by default. You provide a string of MarkDown, you get a string of HTML.",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint    ./.eslintrc.js    ./tests/*.js    ./gulpfile.js/**/*.js    ./source/**/*.js    ./default-options.js    ./core.js    ./index.js",
    "_testWithVerySimpleMarkdowns": "node    ./tests/index.js",
    "_testWithDepPackageExamples": "gulp    --silent   buildExamplesOnce",
    "_testAllWithoutLintingWithoutBrowser": "npm  run  _testWithDepPackageExamples    &&    npm  run  _testWithVerySimpleMarkdowns",
    "test": "npm    run  eslint    &&    npm  run  _testAllWithoutLintingWithoutBrowser",
    "start": "npm   run  eslint    &&    gulp  --silent",
    "start2": "npm  run  eslint    &&    gulp",
    "beforeGitCommit": "npm  test",
    "prepublishOnly": "npm   test"
  },
  "keywords": [
    "markdown",
    "convert",
    "conversion",
    "html",
    "article",
    "pretty",
    "beautify",
    "gorgeous",
    "responsive"
  ],
  "directories": {
    "source": "source",
    "test": "tests",
    "doc": "documents"
  },
  "homepage": "https://github.com/wulechuan/wulechuan-js-generate-html-via-markdown#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wulechuan/wulechuan-js-generate-html-via-markdown.git"
  },
  "bugs": {
    "url": "https://github.com/wulechuan/wulechuan-js-generate-html-via-markdown/issues"
  },
  "dependencies": {
    "@wulechuan/meaningful-error-messages": "^0.1.4",
    "chalk": "^2.4.2",
    "markdown-it": "^9.1.0",
    "markdown-it-anchor": "^5.2.5",
    "markdown-it-checkbox": "^1.1.0",
    "markdown-it-highlightjs": "^3.1.0",
    "markdown-it-toc-done-right": "^4.1.0"
  },
  "devDependencies": {
    "@wulechuan/css-stylus-markdown-themes": "^6.4.5",
    "@wulechuan/gulp-classical-task-cycle": "^0.3.0",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-pipe": "^1.0.4",
    "gulp-rename": "^1.4.0",
    "plugin-error": "^1.0.1",
    "replace-ext": "^1.0.0",
    "through2": "^3.0.1"
  }
}
